<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    tools:context="com.example.admin.kakawev2.Anadir_Comunidad.AnadirDomicilioFragment">

    <TextView
        android:id="@+id/tv_anadirDomicilio_crear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="crear" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">


        <EditText
            android:id="@+id/et_anadirDomicilio_piso"
            style="@style/et_main"
            android:hint="Piso" />


        <EditText
            android:id="@+id/et_anadirDomicilio_puerta"
            style="@style/et_main"
            android:hint="Puerta" />


        <Button
            android:id="@+id/bt_anadirDomicilio_continuar"
            style="@style/bt_main"
            android:layout_marginTop="50dp"
            android:backgroundTint="@color/rojo"
            android:text="Entrar" />

    </LinearLayout>

</LinearLayout>
