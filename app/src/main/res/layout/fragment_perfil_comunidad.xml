<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:layout_marginTop="40dp"
    tools:context="com.example.admin.kakawev2.PerfilComunidadFragment">

    <ImageView
        android:id="@+id/im_perfilCom_imagenComunidad"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/common_google_signin_btn_icon_dark_normal" />

    <TextView
        style="@style/textosLogin"
        android:text="Mi Domicilio" />

    <TextView
        android:id="@+id/textView2"
        style="@style/textosLogin"
        android:text="Piso" />

    <TextView
        android:id="@+id/textView3"
        style="@style/textosLogin"
        android:text="Puerta" />
</LinearLayout>
